<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/rqd_Title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="@string/q_title"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:background="@color/suggestion_highlight_text" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_margin="30dp"
            android:gravity="center_vertical"
            android:orientation="vertical"
            android:padding="15dp"
            android:scrollbars="vertical" >

            <RelativeLayout
                android:id="@+id/strength_output_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_margin="20dp" >

                <TextView
                    android:id="@+id/strength_results_strength_label_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="@string/resultsLabelIconicFont" />

                <TextView
                    android:id="@+id/strength_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/strength_results_strength_label_icon"
                    android:text="@string/title_section_strenght"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/strength_label"
                    android:background="@drawable/text_box"
                    android:padding="10dp" >

                    <TextView
                        android:id="@+id/strength_results_strength"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rqd_output_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_margin="20dp" >

                <TextView
                    android:id="@+id/rqd_results_rqd_label_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="@string/resultsLabelIconicFont" />

                <TextView
                    android:id="@+id/rqd_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/rqd_results_rqd_label_icon"
                    android:text="@string/title_section_rqd"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rqd_label"
                    android:background="@drawable/text_box"
                    android:padding="10dp" >

                    <TextView
                        android:id="@+id/rqd_results_rqd"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/spacing_output_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_margin="20dp" >

                <TextView
                    android:id="@+id/spacing_results_spacing_label_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="@string/resultsLabelIconicFont" />

                <TextView
                    android:id="@+id/spacing_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/spacing_results_spacing_label_icon"
                    android:text="@string/title_section_spacing"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/spacing_label"
                    android:background="@drawable/text_box"
                    android:padding="10dp" >

                    <TextView
                        android:id="@+id/spacing_results_spacing"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/condition_output_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_margin="20dp" >

                <TextView
                    android:id="@+id/condition_results_aperture_label_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="@string/resultsLabelIconicFont" />

                <TextView
                    android:id="@+id/condition_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/condition_results_aperture_label_icon"
                    android:text="@string/title_section_condition"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/condition_label"
                    android:background="@drawable/text_box"
                    android:padding="10dp" >

                    <TextView
                        android:id="@+id/condition_results_aperture"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/groundwater_output_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_margin="20dp" >

                <TextView
                    android:id="@+id/groundwater_results_groundwater_label_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="@string/resultsLabelIconicFont" />

                <TextView
                    android:id="@+id/groundwater_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/groundwater_results_groundwater_label_icon"
                    android:text="@string/title_section_groundwater"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/groundwater_label"
                    android:background="@drawable/text_box"
                    android:padding="10dp" >

                    <TextView
                        android:id="@+id/groundwater_results_groundwater"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/orientation_output_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_margin="20dp" >

                <TextView
                    android:id="@+id/orientation_results_orientation_label_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="@string/resultsLabelIconicFont" />

                <TextView
                    android:id="@+id/orientation_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/orientation_results_orientation_label_icon"
                    android:text="@string/title_section_orientation"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/orientation_label"
                    android:background="@drawable/text_box"
                    android:padding="10dp" >

                    <TextView
                        android:id="@+id/orientation_results_orientation"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        />
                </LinearLayout>
            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>

</ScrollView>